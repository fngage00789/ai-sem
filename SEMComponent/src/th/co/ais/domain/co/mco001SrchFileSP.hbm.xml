<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.co">
	<class name="th.co.ais.domain.co.Mco001SrchFileSP" entity-name="srchFile">
		<id name="rowId" type="java.lang.String"/>
		<property name="seqNo" type="java.lang.String"/>
		<property name="contractDocType" type="java.lang.String"/>
		<property name="contractDocTypeName" type="java.lang.String"/>
		<property name="filePath" type="java.lang.String"/>
		<property name="fileName" type="java.lang.String"/>
		<property name="updateBy" type="java.lang.String"/>
		<property name="updateDt" type="java.util.Date"/>
	</class>
	
	<class name="th.co.ais.domain.co.Mco001Exp" entity-name="srchDDLDoc">
        <id name="rowId" type="java.lang.String"/>
        <property name="title" type="java.lang.String"/>
    </class>
    
    <class name="th.co.ais.domain.co.Eco001ExpContract" entity-name="expContractDoc">
        <id name="SITE_INFO_ID" type="java.lang.String"/>
        <property name="SITE_NAME" type="java.lang.String"/>
        <property name="CONTRACT_NO" type="java.lang.String"/>
    </class>
	
	<!-- << classes declarement -->
    
    <!-- ============================================================================== -->
    <!-- ============================================================================== -->
    
    <!-- >> queries declarement -->
	
	<sql-query name="mco001SrchFile" callable="true">
		<return class="th.co.ais.domain.co.Mco001SrchFileSP" entity-name="srchFile">
			<return-property name="rowId" column="CONTRACT_FILE_ID"/>
			<return-property name="seqNo" column="SEQ_NO"/>
			<return-property name="contractDocType" column="CONTRACT_DOC_TYPE"/>
			<return-property name="contractDocTypeName" column="CONTRACT_DOC_TYPE_NAME"/>
			<return-property name="filePath" column="FILE_PATH"/>
			<return-property name="fileName" column="FILE_NAME"/>
			<return-property name="updateBy" column="UPDATE_BY"/>
			<return-property name="updateDt" column="UPDATE_DT"/>
		</return>
		<![CDATA[ call SEM.SEM_SP_MCO001_SRCH_FILE (?, :contractId, :internalFlag, :groupNo)]]>
	</sql-query>
	
	<sql-query name="mco001SrchDDLDoc" callable="true">
        <return class="th.co.ais.domain.co.Mco001Exp" entity-name="srchDDLDoc">
            <return-property name="rowId" column="LOV_CODE"/>
            <return-property name="title" column="LOV_NAME"/>
        </return>
        <![CDATA[ call SEM.SEM_SP_MCO001_SRCH_DDL_DOC (?, :company)]]>
    </sql-query>
    
    <sql-query name="mco001SrchDDLDocNew" callable="true">
        <return class="th.co.ais.domain.co.Mco001Exp" entity-name="srchDDLDoc">
            <return-property name="rowId" column="CONTRACT_FORMAT_ID"/>
            <return-property name="title" column="CONTRACT_FORMAT_NAME"/>
        </return>
        <![CDATA[ call SEM.SEM_SP_MCO001_SRCH_DDL_DOC_NEW (?)]]>
    </sql-query>
    
    <sql-query name="eco001ExpContractDoc" callable="true">
        <return class="th.co.ais.domain.co.Eco001ExpContract" entity-name="expContractDoc">
            <return-property name="SITE_INFO_ID" column="SITE_INFO_ID"/>
            <return-property name="SITE_NAME" column="SITE_NAME"/>
            <return-property name="CONTRACT_NO" column="CONTRACT_NO"/>
        </return>
        <![CDATA[ call SEM.SEM_PG_ECO002_GET_DATA(:CONTRACT_NO, :CONTRACT_TYPE)]]>
    </sql-query>
</hibernate-mapping>