<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.co">
	<class name="th.co.ais.domain.co.Mco001SrchLovStatusSP">
		<id name="rowId" type="java.lang.String"/>
		<property name="lovCode" type="java.lang.String"/>
		<property name="lovName" type="java.lang.String"/>
		
	</class>
	<sql-query name="mco001SrchLovStatus" callable="true">
		<return class="th.co.ais.domain.co.Mco001SrchLovStatusSP">
			<return-property name="rowId" column="ROW_ID"/>
			<return-property name="lovCode" column="LOV_CODE" />
			<return-property name="lovName" column="LOV_NAME"/>
		</return>
		<![CDATA[ call SEM.SEM_SP_MCO001_SRCH_LOV_STATUS (?, :contractId, :contractStatusId)]]>
	</sql-query>
</hibernate-mapping>