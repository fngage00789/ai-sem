<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.co">
	<class name="th.co.ais.domain.co.Mco004Act" entity-name="mco004Act">
		<id name="pResult" type="java.lang.String" />
		<property name="pRemark" type="java.lang.String" />
		<property name="groupNo" type="java.lang.String" />
	</class>
	
	<sql-query name="mco004Act" callable="true">
		<return class="th.co.ais.domain.co.Mco004Act" entity-name="mco004Act">
			<return-property name="pResult" column="P_RESULT" />
			<return-property name="pRemark" column="P_REMARK" />
			<return-property name="groupNo" column="P_MAX_GROUP_NO" />
		</return>
		<![CDATA[ call SEM.SEM_SP_MCO004_ACT(?, :siteInfoId,:actionType,:userId)]]>
	</sql-query>
</hibernate-mapping>