<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.co">
	<class name="th.co.ais.domain.co.Mco004DefaultDutySP">
		<id name="rowId" type="java.lang.String"/>
		<property name="dutyAmt" type="java.lang.Double"/>
		<property name="copyDuty" type="java.lang.Integer"/>
		<property name="copyDutyAmt" type="java.lang.Double"/>
		<property name="dutyPayDate" type="java.util.Date"/>
		
	</class>
	<sql-query name="mco004DefaultDuty" callable="true">
		<return class="th.co.ais.domain.co.Mco004DefaultDutySP">
			<return-property name="rowId" column="ROW_ID" />
			<return-property name="dutyAmt" column="DUTY_AMT" />
			<return-property name="copyDuty" column="COPY_DUTY"/>
			<return-property name="copyDutyAmt" column="COPY_DUTY_AMT" />
			<return-property name="dutyPayDate" column="DUTY_PAY_DT" />
		</return>
		<![CDATA[ call SEM.SEM_SP_MCO004_DEFAULT_DUTY (?, :siteInfoId)]]>
	</sql-query>
</hibernate-mapping>