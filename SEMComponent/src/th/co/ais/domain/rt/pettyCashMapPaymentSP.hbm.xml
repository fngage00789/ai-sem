<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.rt" >
	<class name="PettyCashMapPaymentSP" >
		<id name="rowId" type="java.lang.String"/>
		<property name="contractNo" type="java.lang.String" />
		<property name="siteName" type="java.lang.String" />
		<property name="effectiveDt" type="java.util.Date" />
		<property name="expireDt" type="java.util.Date" />
		<property name="expenseType" type="java.lang.String" />
		<property name="expenseTypeName" type="java.lang.String" />
		<property name="dueDt" type="java.util.Date" />
		<property name="vendorId" type="java.lang.String" />
		<property name="vendorName" type="java.lang.String" />
		<property name="payeeName" type="java.lang.String" />
	</class>
	
	<sql-query name="spMrt008SrchMapPay" callable="true">
		<return class="th.co.ais.domain.rt.PettyCashMapPaymentSP" >
			<return-property name="rowId" column="PETTY_CASH_PAY_ID" />
			<return-property name="contractNo" column="CONTRACT_NO" />
			<return-property name="siteName" column="SITE_NAME" />
			<return-property name="effectiveDt" column="EFFECTIVE_DT" />
			<return-property name="expireDt" column="EXPIRE_DT" />
			<return-property name="expenseType" column="EXPENSE_TYPE" />
			<return-property name="expenseTypeName" column="EXPENSE_TYPE_NAME" />
			<return-property name="dueDt" column="DUE_DT" />
			<return-property name="vendorId" column="VENDOR_ID" />
			<return-property name="vendorName" column="VENDOR_NAME" />
			<return-property name="payeeName" column="PAYEE_NAME" />
			
		</return>
		<![CDATA[ call SEM.SEM_SP_MRT008_SRCH_MAP_PAY(?, :company, :rowId) ]]>
	</sql-query>
</hibernate-mapping>
