		<property name="pettyCashPayStatusName" type="java.lang.String" />
		<property name="remark" type="java.lang.String" />
		<property name="rentalPaymentId" type="java.lang.String" />
		<property name="recordStatus" type="java.lang.String" />
		<property name="version" type="java.lang.Long" />
		<property name="refClrBatchNo" type="java.lang.String" />
