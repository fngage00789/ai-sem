			<return-property name="exportDt" column="EXPORT_DT" />
			<return-property name="exportFlag" column="EXPORT_FLAG" />
			
		</return>
	<![CDATA[ call SEM.SEM_SP_MRT008_SRCH_PAY(?, :company, :pettyCashPayNo, :region, :requestName, 
									  :requestType, :requestSubject, :requestDtFrom, :requestDtTo, 
									  :contractNo, :siteName, :dueDtFrom, :dueDtTo, :pettyCashPayStatus,
									  :refClrBatchNo, :category) ]]>
	</sql-query>
	
