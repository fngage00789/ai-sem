		<property name="excRequestAmt" type="java.lang.Double" />
		<property name="receiptTaxFlag" type="java.lang.String" />
		<property name="clrReceiptTaxFlag" type="java.lang.String" />
		<property name="clrReceiptTaxFlagName" type="java.lang.String" />
		<property name="requestDt" type="java.util.Date" />
		<property name="contractNo" type="java.lang.String" />
		<property name="siteName" type="java.lang.String" />
		<property name="effectiveDt" type="java.util.Date" />
		<property name="expireDt" type="java.util.Date" />
		<property name="expenseType" type="java.lang.String" />
