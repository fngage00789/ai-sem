		<return class="th.co.ais.domain.rt.PettyCashPaySP" entity-name="updateMRT008">  
			<return-property name="statusResult" column="P_RESULT" />
			<return-property name="refClrBatchNo" column="REF_CLR_BATCH_NO" />
			<return-property name="exportDt" column="EXPORT_DT" />
		</return>
	<![CDATA[ call SEM.SEM_SP_MRT008_UPD_PAY_EXPORT(?, :pettyCashPayIds, :username) ]]>
