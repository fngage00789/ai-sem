	<class name="th.co.ais.domain.rt.PettyCashPaySP" entity-name="updateMRT008" >
		<id name="statusResult" type="java.lang.String" />
		<property name="exportDt" type="java.util.Date" />
		<property name="refClrBatchNo" type="java.lang.String" />
	</class>
	
	<sql-query name="spMrt008SrchPettyCashPay" callable="true">
		<return class="th.co.ais.domain.rt.PettyCashPaySP" entity-name="searchPettyCash">
			<return-property name="rowId" column="PETTY_CASH_PAY_ID" />
			<return-property name="company" column="COMPANY"/>
			<return-property name="pettyCashPayNo" column="PETTY_CASH_PAY_NO"/>
			<return-property name="requestName" column="REQUEST_NAME" />
			<return-property name="requestSubject" column="REQUEST_SUBJECT" />
			<return-property name="requestAmt" column="REQUEST_AMT" />
			<return-property name="vatTypeName" column="VAT_TYPE_NAME" />
			<return-property name="vatType" column="VAT_TYPE" />
			<return-property name="vatRate" column="VAT_RATE" />
			<return-property name="vatAmt" column="VAT_AMT" />
			<return-property name="incRequestAmt" column="INC_REQUEST_AMT" />
			<return-property name="excRequestAmt" column="EXC_REQUEST_AMT" />
