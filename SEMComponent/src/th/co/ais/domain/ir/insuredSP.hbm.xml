<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="th.co.ais.domain.ir">
	<class name="th.co.ais.domain.ir.InsuredSP" >
		<id name="rowId" type="java.lang.String" />
		<property name="insuredId" type="java.lang.String" />
		<property name="networkType" type="java.lang.String" />
		<property name="company" type="java.lang.String" />
		<property name="locationId" type="java.lang.String" />
		<property name="locationName" type="java.lang.String" />
		<property name="insuredAmt" type="java.lang.String" />
		<property name="effDt" type="java.sql.Date" />
		<property name="updateBy" type="java.lang.String" />
		<property name="updateDt" type="java.sql.Date" />
		<property name="ntCode" type="java.lang.String" />
		<property name="cCode" type="java.lang.String" />
	</class>
	<sql-query name="queryInsuredSP" callable="true">
		<return class="th.co.ais.domain.ir.InsuredSP">
			<return-property name="rowId" column="ROW_ID" />
			<return-property name="insuredId" column="SEM_IR_INSURED_ID" />
			<return-property name="networkType" column="NETWORK_TYPE" />
			<return-property name="company" column="COMPANY" />
			<return-property name="locationId" column="LOCATION_ID" />
			<return-property name="locationName" column="LOCATION_NAME" />
			<return-property name="effDt" column="EFF_DT" />
			<return-property name="insuredAmt" column="INSURED_AMT" />
			<return-property name="updateBy" column="UPDATE_BY" />
			<return-property name="updateDt" column="UPDATE_DT" />
			<return-property name="ntCode" column="NT_CODE" />
			<return-property name="cCode" column="C_CODE" />
		</return>
		 <![CDATA[ call SEM.SEM_SP_MIR005_SRCH(?, :company, :networkType, :locationId, :effDt) ]]>
	</sql-query>
</hibernate-mapping>


