<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="th.co.ais.domain.ir">
	<class name="th.co.ais.domain.ir.Mir004SP" entity-name="mir004Srch">
		<id name="rowId" type="java.lang.String" />
		<property name="networkType" type="java.lang.String" />
		<property name="networkTypeDesc" type="java.lang.String" />
		<property name="company" type="java.lang.String" />
		<property name="transferType" type="java.lang.String" />
		<property name="transferTypeDesc" type="java.lang.String" />
		<property name="lossType" type="java.lang.String" />
		<property name="lossTypeDesc" type="java.lang.String" />
		<property name="deductAmt" type="java.lang.Double" />
		<property name="effDate" type="java.sql.Date" />
		<property name="updateBy" type="java.lang.String" />
		<property name="updateDt" type="java.sql.Date" />
	</class>
	
	<class name="th.co.ais.domain.ir.Mir004SP" entity-name="mir004Edt">
		<id name="result" type="java.lang.String" />
		<property name="msgError" type="java.lang.String" />
	</class>
	
	<sql-query name="queryDeductible" callable="true">
		<return class="th.co.ais.domain.ir.Mir004SP" entity-name="mir004Srch">
			<return-property name="rowId" column="DEDUCT_ID" />
			<return-property name="networkType" column="NETWORK_TYPE" />
			<return-property name="networkTypeDesc" column="NETWORK_TYPE_DESC" />
			<return-property name="company" column="COMPANY" />
			<return-property name="transferType" column="TRANSFER_TYPE" />
			<return-property name="transferTypeDesc" column="TRANSFER_TYPE_DESC" />
			<return-property name="lossType" column="LOSS_TYPE" />
			<return-property name="lossTypeDesc" column="LOSS_TYPE_DESC" />
			<return-property name="deductAmt" column="DEDUCT_AMT" />
			<return-property name="effDate" column="EFFECTIVE_DT" />
			<return-property name="updateBy" column="UPDATE_BY" />
			<return-property name="updateDt" column="UPDATE_DT" />
		</return>
		 <![CDATA[ call SEM.SEM_SP_MIR004_SRCH(?, :company, :networkType, :transferType, :effDateFrom, :effDateTo) ]]>
	</sql-query>
	
	<sql-query name="queryDeductibleEdt" callable="true">
		<return class="th.co.ais.domain.ir.Mir004SP" entity-name="mir004Edt">
			<return-property name="result" column="P_RESULT" />
			<return-property name="msgError" column="P_REMARK" />
		</return>
		 <![CDATA[ call SEM.SEM_SP_MIR004_EDT(?, :rowId, :company, :networkType, :transferType, :lossType, :deductAmt, :effDate, :currentUser) ]]>
	</sql-query>
</hibernate-mapping>


