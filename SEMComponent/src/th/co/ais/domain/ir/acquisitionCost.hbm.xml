<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="th.co.ais.domain.ir">
	<class name="th.co.ais.domain.ir.AcquisitionCostSP" >
		<id name="rowId" type="java.lang.String" />
		<property name="networkType" type="java.lang.String" />
		<property name="company" type="java.lang.String" />
		<property name="tfType" type="java.lang.String" />
		<property name="totLocation" type="java.lang.String" />
		<property name="acqAmt" type="java.lang.String" />
		<property name="asOfMonth" type="java.lang.String" />
		<property name="networkCode" type="java.lang.String" />
		<property name="companyCode" type="java.lang.String" />
		<property name="tfCode" type="java.lang.String" />
	</class>
	<sql-query name="queryAcquisitionCost" callable="true">
		<return class="th.co.ais.domain.ir.AcquisitionCostSP">
			<return-property name="rowId" column="ROW_ID" />
			<return-property name="networkType" column="NETWORK_TYPE" />
			<return-property name="company" column="COMPANY" />
			<return-property name="tfType" column="TF_TYPE" />
			<return-property name="totLocation" column="TOT_LOCATION" />
			<return-property name="acqAmt" column="ACQ_AMT" />
			<return-property name="asOfMonth" column="AS_OF_MONTH" />
			<return-property name="networkCode" column="NT_CODE" />
			<return-property name="companyCode" column="C_CODE" />
			<return-property name="tfCode" column="TF_CODE" />
		</return>
		 <![CDATA[ call SEM.SEM_SP_MIR001_SRCH(?, :company, :networkType, :tfType, :effDt, :strParam) ]]>
	</sql-query>
</hibernate-mapping>


