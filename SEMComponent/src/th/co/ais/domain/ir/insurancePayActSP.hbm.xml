<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.ir">
	<class name="th.co.ais.domain.ir.InsurancePayActSP">
		<id name="resultMsg" type="java.lang.String"/>
		<property name="pRemark" type="java.lang.String"/>
	</class>
	<sql-query name="mir010Act" callable="true">
		<return class="th.co.ais.domain.ir.InsurancePayActSP">
			<return-property name="resultMsg" column="P_RESULT" />
			<return-property name="pRemark" column="P_REMARK" />
		</return>
		<![CDATA[ call SEM.SEM_SP_MIR010_ACT(?, :rowId, :actionType, :remark, :userId) ]]>
	</sql-query>
</hibernate-mapping>