<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.si">
	<class name="th.co.ais.domain.si.Msi001ChkDocNo" entity-name="chkDocNo">
		<id name="result" type="java.lang.String"/>
	</class>
	<class name="th.co.ais.domain.si.Msi001ChkDocNo" entity-name="msi001Save">
        <id name="result" type="java.lang.String"/>
    </class>
	<sql-query name="msi001ChkDocNo" callable="true">
		<return class="th.co.ais.domain.si.Msi001ChkDocNo" entity-name="chkDocNo">
			<return-property name="result" column="status_msg" />
		</return>
		<![CDATA[ call SEM.SEM_SP_MSI001_CHK_DOC_NO(?, :siteApproveId, :docNo, :reqType, :contractNo) ]]>
	</sql-query>
	<sql-query name="msi001Save" callable="true">
        <return class="th.co.ais.domain.si.Msi001ChkDocNo" entity-name="msi001Save">
            <return-property name="result" column="status_msg" />
        </return>
        <![CDATA[ call SEM.SEM_SP_MSI001_SAVE(?, :siteApproveId, :userId) ]]>
    </sql-query>
</hibernate-mapping>