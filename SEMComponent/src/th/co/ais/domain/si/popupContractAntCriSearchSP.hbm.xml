<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.si">
	<class name="th.co.ais.domain.si.PopupContractAntCriSearchSP">
		<id name="siteInfoId" type="java.lang.String"/>
		<property name="contractId" type="java.lang.String"/>
		<property name="contractNo" type="java.lang.String"/>
		<property name="renewNo" type="java.lang.String"/>
		<property name="siteName" type="java.lang.String"/>
		<property name="region" type="java.lang.String"/>
		<property name="company" type="java.lang.String"/>
		<property name="effDate" type="java.util.Date"/>
		<property name="expDate" type="java.util.Date"/>
	</class>
	
	<sql-query name="queryPopupContractAntCriSearch" callable="true">
		<return class="th.co.ais.domain.si.PopupContractAntCriSearchSP">
			<return-property name="siteInfoId" column="SITE_INFO_ID" />
			<return-property name="contractId" column="CONTRACT_ID" />
			<return-property name="contractNo" column="CONTRACT_NO" />
			<return-property name="renewNo" column="RENEW_NO" />
			<return-property name="siteName" column="SITE_NAME" />
			<return-property name="region" column="REGION" />
			<return-property name="company" column="COMPANY" />
			<return-property name="effDate" column="EFFECTIVE_DT" />
			<return-property name="expDate" column="EXPIRE_DT" />
		</return>
		<![CDATA[ call SEM.SEM_SP_PSI004_SRCH (?, :company, :contractNo, :siteName, :vendorName, :vendorAddress, :payeeName, :payeeAddress) ]]>
	</sql-query>
	
</hibernate-mapping>