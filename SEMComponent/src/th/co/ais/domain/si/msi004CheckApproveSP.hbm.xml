<hibernate-mapping package="th.co.ais.domain.si">
	<class name="th.co.ais.domain.si.Msi004CheckApproveSP">
		<id name="resultMsg" type="java.lang.String"/>
		<property name="message" type="java.lang.String"/>
		
	</class>
	<sql-query name="msi004CheckApprove" callable="true">
