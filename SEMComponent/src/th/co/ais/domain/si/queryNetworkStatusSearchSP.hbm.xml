<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.si">

	<class name="th.co.ais.domain.si.QueryNetworkStatusSearchSP">
	
		<id name="rowId" type="java.lang.String"/>
		<property name="locationId" type="java.lang.String"/>
		<property name="locationCode" type="java.lang.String"/>
		<property name="siteStatus" type="java.lang.String"/>
		<property name="networkStatus" type="java.lang.String"/>
		<property name="pmsStatus" type="java.lang.String"/>
		<property name="contractNo" type="java.lang.String"/>
	</class>
	<sql-query name="querySearchQueryNetworkStatus" callable="true">
	<return class="th.co.ais.domain.si.QueryNetworkStatusSearchSP">
		<return-property name="rowId" column="LOCATION_ID" />
		
		<return-property name="locationId" column="LOCATION_ID" />
		<return-property name="locationCode" column="LOCATION_CODE" />
		<return-property name="siteStatus" column="SITE_STATUS" />
		<return-property name="networkStatus" column="NETWORK_STATUS" />
		<return-property name="pmsStatus" column="PMS_STATUS" />
		<return-property name="contractNo" column="CONTRACT_NO" />
	 </return>
		<![CDATA[ call SEM.SEM_SP_QSI002_SRCH(?, :locationId, :locationCode, :siteStatus, :networkStatus, :pmsStatus, :contractNo) ]]>
	</sql-query>
</hibernate-mapping>