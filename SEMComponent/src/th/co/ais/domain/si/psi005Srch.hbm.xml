<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.si">
	<class name="th.co.ais.domain.si.Psi005Srch">
		<id name="contractId" type="java.lang.String"/>
		<property name="contractNo" type="java.lang.String"/>
		<property name="siteName" type="java.lang.String"/>
		<property name="company" type="java.lang.String"/>
		<property name="region" type="java.lang.String"/>
		<property name="siteInfoId" type="java.lang.String"/>
		<property name="effDate" type="java.util.Date"/>
		<property name="expDate" type="java.util.Date"/>
		<property name="sendRenewId" type="java.lang.String"/>
		<property name="seqNo" type="java.lang.String"/>
		<property name="cycleNo" type="java.lang.String"/>
		<property name="docNo" type="java.lang.String"/>
		<property name="docDate" type="java.util.Date"/>
		<property name="reqType" type="java.lang.String"/>
		<property name="locationId" type="java.lang.String"/>
	</class>
	<sql-query name="queryPopupContractSearchPSI005" callable="true">
		<return class="th.co.ais.domain.si.Psi005Srch">
			<return-property name="contractId" column="CONTRACT_ID" />
			<return-property name="contractNo" column="CONTRACT_NO" />
			<return-property name="siteName" column="SITE_NAME" />
			<return-property name="company" column="COMPANY" />
			<return-property name="region" column="REGION" />
			<return-property name="siteInfoId" column="SITE_INFO_ID" />
			<return-property name="effDate" column="EFFECTIVE_DT" />
			<return-property name="expDate" column="EXPIRE_DT" />
			<return-property name="sendRenewId" column="SEND_RENEW_ID" />
			<return-property name="seqNo" column="SEQ_NO" />
			<return-property name="cycleNo" column="CYCLE_NO" />
			<return-property name="docNo" column="DOC_NO" />
			<return-property name="docDate" column="DOC_DT" />
			<return-property name="reqType" column="REQ_TYPE" />
			<return-property name="locationId" column="LOCATION_ID" />
			
		</return>
		<![CDATA[ call SEM.SEM_SP_PSI005_SRCH (?, :contractNo, :siteName, :company, :reqType, :locationId, :siInfoId) ]]>
	</sql-query>
	
</hibernate-mapping>