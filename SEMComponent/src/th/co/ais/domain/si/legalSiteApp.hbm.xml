<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.si">

	<class name="th.co.ais.domain.si.LegalSiteAppSP" entity-name="legalSiteApp">
		<id name="rowId" type="java.lang.String"/>
		<property name="siteAppId" type="java.lang.String" />
		<property name="batchNo" type="java.lang.String" />
		<property name="region" type="java.lang.String" />
		<property name="company" type="java.lang.String" />
		<property name="docNo" type="java.lang.String" />
		<property name="reqDocType" type="java.lang.String" />
		<property name="reqOfficer" type="java.lang.String" />
		<property name="contractNo" type="java.lang.String" />
		<property name="effectiveDt" type="java.sql.Date" />
		<property name="expireDt" type="java.sql.Date" />
		<property name="locationId" type="java.lang.String" />
		<property name="locationName" type="java.lang.String" />
		<property name="siteDetail" type="java.lang.String" />
		<property name="locationZone" type="java.lang.String" />
		<property name="networkStatus" type="java.lang.String" />
		<property name="flowStatus" type="java.lang.String" />
		<property name="statusDt" type="java.sql.Date" />
		<property name="approveDt" type="java.sql.Date" />
