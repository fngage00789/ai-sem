			<return-property column="APPROVE_DT" name="approveDt"/>
			<return-property column="APPROVE_BY" name="approveBy"/>
			<return-property column="FLOW_REMARK" name="flowRemark"/>
			<return-property column="USER_ID" name="userId"/>
		</return>
