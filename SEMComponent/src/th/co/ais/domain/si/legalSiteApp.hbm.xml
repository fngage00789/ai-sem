		<property name="flowRemark" type="java.lang.String" />
		<property name="userId" type="java.lang.String" />
	</class>
	
	<sql-query name="mlg001Search" callable="true">
		<return class="th.co.ais.domain.si.LegalSiteAppSP" entity-name="legalSiteApp">
			<return-property column="SITE_APP_ID" name="rowId"/>
			<return-property column="SITE_APP_ID" name="siteAppId"/>
			<return-property column="BATCH_NO" name="batchNo"/>
			<return-property column="REGION" name="region"/>
			<return-property column="COMPANY" name="company"/>
			<return-property column="DOC_NO" name="docNo"/>
			<return-property column="REQ_DOC_TYPE" name="reqDocType"/>
