			<return-property column="CONTRACT_NO" name="contractNo"/>
			<return-property column="EFFECTIVE_DT" name="effectiveDt"/>
			<return-property column="EXPIRE_DT" name="expireDt"/>
			<return-property column="LOCATION_ID" name="locationId"/>
			<return-property column="LOCATION_NAME" name="locationName"/>
			<return-property column="SITE_DETAIL" name="siteDetail"/>
			<return-property column="LOCATION_ZONE" name="locationZone"/>
