<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.si" >
	<class name="th.co.ais.domain.si.SendRenewActSP">
		<id name="result" type="java.lang.String"/>
		<property name="remark" type="java.lang.String"/>
	</class>
	<sql-query name="sendRenewAct" callable="true">
		<return class="th.co.ais.domain.si.SendRenewActSP">
			<return-property name="result" column="P_RESULT" />
			<return-property name="remark" column="P_REMARK" />
		</return>
		<![CDATA[ call SEM_SP_MSI005_ACT(?, :siteInfoId, :actionType, :userId) ]]>
	</sql-query>
</hibernate-mapping>