<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.si">
	<class name="th.co.ais.domain.si.Msi004DefaultTotSP">
		<id name="rowId" type="java.lang.String"/>
		<property name="totSendFlag" type="java.lang.String"/>
		
	</class>
	<sql-query name="msi004DefaultTot" callable="true">
		<return class="th.co.ais.domain.si.Msi004DefaultTotSP">
			<return-property name="rowId" column="ROW_ID" />
			<return-property name="totSendFlag" column="TOT_SEND_FLAG"/>
		</return>
		<![CDATA[ call SEM.SEM_SP_MSI004_DEFAULT_TOT (?, :siteApproveId) ]]>
	</sql-query>
</hibernate-mapping>