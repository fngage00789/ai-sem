<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.si">
	<class name="th.co.ais.domain.si.ApproveSiteInfoSP" entity-name="approveSiteInfo">
		<id name="resultMsg" type="java.lang.String"/>
		
	</class>
	<class name="th.co.ais.domain.si.ApproveSiteInfoSP" entity-name="approveEL">
		<id name="resultMsg" type="java.lang.String"/>
		
	</class>
	
	<sql-query name="approveSiteInfo" callable="true">
		<return class="th.co.ais.domain.si.ApproveSiteInfoSP" entity-name="approveSiteInfo">
			<return-property name="resultMsg" column="P_RESULT" />
		</return>
		<![CDATA[ call SEM.SEM_SP_APPROVE_SITE_INFO (?, :siteInfoId, :screen, :currentUser) ]]>
	</sql-query>
	
	<sql-query name="approveEL" callable="true">
		<return class="th.co.ais.domain.si.ApproveSiteInfoSP" entity-name="approveEL">
			<return-property name="resultMsg" column="P_RESULT" />
		</return>
		<![CDATA[ call SEM.SEM_SP_APPROVE_EL (?, :siteInfoId, :screen, :currentUser) ]]>
	</sql-query>
</hibernate-mapping>