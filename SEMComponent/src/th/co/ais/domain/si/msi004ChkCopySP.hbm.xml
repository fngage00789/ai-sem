<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.si">
	<class name="th.co.ais.domain.si.Msi004ChkCopySP" entity-name="msi004ChkCopy">
		<id name="result" type="java.lang.String"/>
		<property name="errorMsg" type="java.lang.String"/>
	</class>
	
	<class name="th.co.ais.domain.si.Msi004ChkCopySP" entity-name="msi004ChkAlert">
		<id name="result" type="java.lang.String"/>
		<property name="errorMsg" type="java.lang.String"/>
	</class>
	
	<sql-query name="msi004ChkCopy" callable="true">
		<return class="th.co.ais.domain.si.Msi004ChkCopySP" entity-name="msi004ChkCopy">
			<return-property name="result" column="P_RESULT" />
			<return-property name="errorMsg" column="MESSAGE" />
		</return>
		<![CDATA[ call SEM.SEM_SP_MSI004_CHK_COPY(?, :contractNo) ]]>
	</sql-query>
	
	<sql-query name="msi004ChkAlert" callable="true">
		<return class="th.co.ais.domain.si.Msi004ChkCopySP" entity-name="msi004ChkAlert">
			<return-property name="result" column="P_RESULT" />
			<return-property name="errorMsg" column="P_REMARK" />
		</return>
		<![CDATA[ call SEM.SEM_SP_MSI004_CHK_ALERT(?, :docApproveId) ]]>
	</sql-query>
</hibernate-mapping>