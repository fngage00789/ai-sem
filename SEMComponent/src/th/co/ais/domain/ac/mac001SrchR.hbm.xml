<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.ac">
	<class name="th.co.ais.domain.ac.Mac001SrchR">
		<id name="paymentStatusDesc" type="java.lang.String"/>
		<property name="paymentStatusDt" type="java.sql.Date"/>
		<property name="reason" type="java.lang.String"/>
	</class>
	<sql-query name="spMac001SrchR" callable="true">
		<return class="th.co.ais.domain.ac.Mac001SrchR">
			<return-property name="paymentStatusDesc" column="PAYMENT_STATUS_DESC" />
			<return-property name="paymentStatusDt" column="PAYMENT_STATUS_DT" />
			<return-property name="reason" column="REASON" />
			
		</return>
		<![CDATA[ call SEM_SP_MAC001_SRCH_R(?, :transPaymemtId) ]]>
	</sql-query>
</hibernate-mapping>