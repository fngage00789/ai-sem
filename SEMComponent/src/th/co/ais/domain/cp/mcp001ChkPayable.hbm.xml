<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.cp">
	<class name="th.co.ais.domain.cp.Mcp001ChkPayable">
		<id name="payAbleFlag" type="java.lang.String"/>
		<property name="cancleAbleFlag" type="java.lang.String"/>
	</class> 
	<sql-query name="spMcp001Chkable" callable="true">
	<return class="th.co.ais.domain.cp.Mcp001ChkPayable">
		<return-property name="payAbleFlag" column="PAYABLE_FLAG" />
		<return-property name="cancleAbleFlag" column="CANCELPAYABLE_FLAG" />
	 </return>
	 <![CDATA[call SEM_SP_MCP001_CHK_PAYABLE(?, :rowId)]]>
	</sql-query>
</hibernate-mapping>