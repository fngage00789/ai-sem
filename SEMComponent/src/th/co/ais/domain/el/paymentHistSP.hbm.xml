			<property name="fromTermOfPaymentDt" type="java.util.Date"/>	
			<property name="toTermOfPaymentDt" type="java.util.Date"/>	
			<property name="pRead" type="java.lang.String"/>	
			<property name="lRead" type="java.lang.String"/>	
