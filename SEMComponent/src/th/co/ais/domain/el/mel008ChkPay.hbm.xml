<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="th.co.ais.domain.el">
	<class name="th.co.ais.domain.el.MEL008ChkPay">
		<id name="pResult" type="java.lang.String"/>
		<property name="pRemark" type="java.lang.String"/>
				
	</class>
	
	<sql-query name="mel008ChkPay" callable="true">
		<return class="th.co.ais.domain.el.MEL008ChkPay">  
			<return-property name="pResult" column="P_RESULT"/>
			<return-property name="pRemark" column="P_REMARK"/>
			
		</return>
	<![CDATA[ call SEM_SP_MEL008_CHK_PAY(?, :paymentId) ]]>
	</sql-query>
	
</hibernate-mapping>
