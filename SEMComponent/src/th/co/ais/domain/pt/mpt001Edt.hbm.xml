<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.pt">
	<class name="th.co.ais.domain.pt.Mpt001Edt">
		<id name="resultMsg" type="java.lang.String"/>
		<property name="pRemark" type="java.lang.String"/>
	</class>
	<sql-query name="spMpt001Edt" callable="true">
		<return class="th.co.ais.domain.pt.Mpt001Edt">
			<return-property name="resultMsg" column="P_RESULT" />
			<return-property name="pRemark" column="P_REMARK" />
		</return>
		<![CDATA[ call SEM_SP_MPT001_EDT(?, :actionType, :contractNo, :pTaxPayType, :payGovtFlag, :userID) ]]>
	</sql-query>
</hibernate-mapping>