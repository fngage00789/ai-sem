	   <return-property name="vendorCode" column="VENDOR" />
	   <return-property name="vendorName" column="VENDOR_NAME" />
	   <return-property name="vendorAddress1" column="VENDOR_ADDRESS1" />
	   <return-property name="vendorAddress2" column="VENDOR_ADDRESS2" />
	   <return-property name="contractNo" column="CONTRACT_NO"/>
  </return>
  <![CDATA[ call SEM_SP_MPT003_EXP(?, :pTaxYearFrom, :pTaxYearTo, :contractNo, :company, :region, :province, :amphure, :payGovtFlag, :lessorName, :govtName, :pTaxStatus, :payeeName, :pTaxEstmStatus, :paymentStatus) ]]>
 </sql-query>
</hibernate-mapping>
