<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="th.co.ais.domain.pt">
 <class name="th.co.ais.domain.pt.Mpt003ExpLetter">
  <id name="rowId" type="java.lang.String"/>
  <property name="vendorCode" type="java.lang.String"/>
	  <property name="vendorName" type="java.lang.String"/>
	  <property name="vendorAddress1" type="java.lang.String"/>
	  <property name="vendorAddress2" type="java.lang.String"/>
	  <property name="contractNo" type="java.lang.String"/>
 </class>
 
 <sql-query name="spMpt003Exp" callable="true" >
   <return class="th.co.ais.domain.pt.Mpt003ExpLetter" >
