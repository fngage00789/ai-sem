<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="th.co.ais.domain.pt">
	<class name="th.co.ais.domain.pt.Mpt004Edt">
		<id name="resultMsg" type="java.lang.String"/>
		<property name="pRemark" type="java.lang.String"/>
		<property name="ptaxPaymentId" type="java.lang.String"/>
	</class>
	<sql-query name="spMpt004Edt" callable="true">
		<return class="th.co.ais.domain.pt.Mpt004Edt">
			<return-property name="resultMsg" column="P_RESULT" />
			<return-property name="pRemark" column="P_REMARK" />
			<return-property name="ptaxPaymentId" column="PTAX_PAYMENT_ID" />
		</return>
		<![CDATA[ call SEM_SP_MPT004_EDIT(?, :ptaxPaymentId
									 , :contractNo
		                             , cast(:pTaxYear as numeric)
		                             , :expenseType
		                             , :periodNo
		                             , :monthFrom
		                             , :monthTo
		                             , :periodType
		                             ,  cast(:periodInterval as numeric) 
		                             , :docType
		                             , :penaltyType
		                             , :docNo
		                             , cast(:docDt as date)
		                             , :pTaxPayType
		                             , :payGovtFlag
		                             , :vendorCode
		                             , :payeeId
		                             , cast(:estmAmt as numeric)
		                             , cast(:pTaxAmt as numeric)
		                             , cast(:totalAmt as numeric)
		                             , :vatType
		                             , cast(:vatRate as numeric)
		                             , :whtType
		                             , cast(:whtRate as numeric)
		                             , cast(:excAmt as numeric)
		                             , cast(:vatAmt as numeric)
		                             , cast(:incAmt as numeric)
		                             , cast(:whtAmt as numeric)
		                             , cast(:chqAmt as numeric)
		                             , :diffRemark
		                             , :paymentType
		                             , :paymentMethod
		                             , cast(:chqDt as date)
		                             , cast(:chqReceiveDt as date)
		                             , cast(:transferDt as date)
		                             , :remark
		                             , :userId) ]]>
	</sql-query>
</hibernate-mapping>