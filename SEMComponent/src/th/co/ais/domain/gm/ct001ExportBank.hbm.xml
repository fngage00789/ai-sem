<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="th.co.ais.domain.gm">
	<class name="th.co.ais.domain.gm.CT001ExportBank" >
		<id name="rowId" type="java.lang.String" />
		<property name="bankSum" type="java.lang.String"/>
		<property name="company" type="java.lang.String"/>
		<property name="vendorCode" type="java.lang.String"/>
		<property name="vendorName" type="java.lang.String"/>
		<property name="bookBankCode" type="java.lang.String"/>
		<property name="bankAccNo" type="java.lang.String"/>
		<property name="bankName" type="java.lang.String"/>
		<property name="bankAccName" type="java.lang.String"/>
		<property name="remark" type="java.lang.String"/>
		
	</class>
	<sql-query name="queryCT001ExportBank" callable="true">
		<return class="th.co.ais.domain.gm.CT001ExportBank">  
			<return-property name="rowId" column="ROW_ID" />
			<return-property name="bankSum" column="BANK_SUM" />
			<return-property name="company" column="COMPANY" />
			<return-property name="vendorCode" column="VENDOR_CODE" />
			<return-property name="vendorName" column="VENDOR_NAME" />
			<return-property name="bookBankCode" column="BANK_CODE" />
			<return-property name="bankAccNo" column="BANK_ACC_NO" />
			<return-property name="bankName" column="BANK_NAME" />
			<return-property name="bankAccName" column="BANK_ACC_NAME" />
			<return-property name="remark" column="REMARK" />
		</return>
		 <![CDATA[ call SEM_SP_MCT001_EX_BANK(?, :vendorMapPayeeId) ]]>
	</sql-query>
</hibernate-mapping>


