	<!-- << classes declarement -->
    
    <!-- ============================================================================== -->
    <!-- ============================================================================== -->
    
    <!-- >> queries declarement -->
    
	<sql-query name="queryApproveBookBankAct" callable="true">
		<return class="th.co.ais.domain.gm.ApproveBookBankAct" entity-name="queryApproveBookBankAct">  
			<return-property name="resultMsg" column="P_RESULT" />
			<return-property name="pRemark" column="P_REMARK" />
			<return-property name="fileName" column="FILE_NAME" />
		</return>
		 <![CDATA[ call SEM_SP_MCT003_ACT(?, :actionType, :vendorMapPayeeId, :remark, :userId) ]]>
	</sql-query>
	<sql-query name="mct003CheckApproveBookBank" callable="true">
		<return class="th.co.ais.domain.gm.ApproveBookBankAct" entity-name="mct003CheckApproveBookBank">  
