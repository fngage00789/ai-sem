<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="th.co.ais.domain.gm">
	<class name="Mct001Conf" >
		<id name="pResult" type="java.lang.String" />
		<property name="pRemark" type="java.lang.String" />
	</class>
	<sql-query name="spMct001CONF" callable="true">
		<return class="th.co.ais.domain.gm.Mct001Conf">  
			<return-property name="pResult" column="P_RESULT" />
			<return-property name="pRemark" column="P_REMARK" />
		</return>
	    <![CDATA[ call SEM.SEM_SP_MCT001_CONF(?, :vendorMasterId, :userId) ]]>
	</sql-query>
</hibernate-mapping>


